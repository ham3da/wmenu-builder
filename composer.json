{
    "name": "ham3da/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel 5 to 10",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
        {
            "name": "Javad Ehteshami",
            "email": "ham3da.j@gmail.com",
            "role": "Developer"
        },
    	{
            "name": "Rendy Harimayco",
            "email": "rendyharimayco@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Cristian Garcia",
            "email": "informacion@cristiangarcia.co",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.* || 6.* || 7.* || 8.* || 9.* || 10.*"
    },
    "autoload": {
        "psr-4": {
          "Ham3da\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ham3da\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Ham3da\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
